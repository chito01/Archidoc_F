@model List<Archidoc_F.Models.Liste_documents>
@{
    Layout = null;
    ViewData["Title"] = "Repertoire Documents Archivés";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="~/images/archi.ico" sizes="32x32" type="image/png">
    <title>@ViewData["Title"] - Archidoc_F</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>

    <h2>@ViewData["Title"]</h2>

    <div class="table-responsive">
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th scope="col">Code Archive</th>
                    <th scope="col">Nom Document</th>
                    <th scope="col">Categorie Document</th>
                    <th scope="col">Nom Employeur</th>
                    <th scope="col">Type Employeur</th>
                    <th scope="col">Numero Affiliation</th>
                    <th scope="col">Nom Commune</th>
                    <th scope="col">Libelle periode</th>
                    <th scope="col">Libelle Rayon</th>
                    <th scope="col">Service Provenance</th>


                </tr>
            </thead>

            <tbody>
                @foreach (var ab in Model)
                {
                <tr>
                    <td>@ab.Code_Archive</td>
                    <td>@ab.nom_document</td>
                    <td>@ab.type_Document</td>
                    <td>@ab.nom_Employeur</td>
                    <td>@ab.nom_TypeE</td>
                    <td>@ab.numero_Affiliation</td>
                    <td>@ab.nom_commune</td>
                    <td>@ab.Libelle_periode</td>
                    <td>@ab.Libelle_Rayon</td>
                    <td>@ab.nom_service</td>


                </tr>
                }

            </tbody>
        </table>
    </div>


    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Archidoc_F - @ViewData["Title"]
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js"></script>

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

</body>
</html>

