@model List<Archidoc_F.Models.Liste_documents>
@{
    Layout = "_Layout_sectionRec";
    ViewData["Title"] = "Liste des documents";
}
<h2>@ViewData["Title"]</h2>
<a asp-area="" asp-controller="Home" asp-action="Affichage_Do" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm">
    <i class="fas fa-download fa-sm text-white-50"></i> Generate Report
</a>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Numero_document</th>
                <th scope="col">Nom document</th>
                <th scope="col">Acronyme document</th>

            </tr>
        </thead>
       
        <tbody>
            @foreach (var ab in Model) {
        <tr>
            <td>@ab.numero_document</td>
            <td>@ab.nom_document</td>
            <td>@ab.acronyme_document</td>
        </tr>
            }
            
        </tbody>
    </table>
</div>

