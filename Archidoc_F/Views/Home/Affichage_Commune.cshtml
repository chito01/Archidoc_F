@model List<Archidoc_F.Models.Liste_documents>
@{
    Layout = "_Layout_sectionRec";
    ViewData["Title"] = "Liste des communes ou secteurs";
}
<h2>@ViewData["Title"]</h2>
<a asp-area="" asp-controller="Home" asp-action="Affichage_Co" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm">
    <i class="fas fa-download fa-sm text-white-50"></i> Generate Report
</a>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Numero_Commune</th>
                <th scope="col">Nom Commune</th>
                <th scope="col">District</th>

            </tr>
        </thead>
       
        <tbody>
            @foreach (var ab in Model) {
        <tr>
            <td>@ab.numero_commune</td>
            <td>@ab.nom_commune</td>
            <td>@ab.District</td>
        </tr>
            }
            
        </tbody>
    </table>
</div>

