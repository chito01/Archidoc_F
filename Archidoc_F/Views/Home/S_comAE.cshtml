@model List<Archidoc_F.Models.Liste_documents>
@{
    Layout = "_Layout_sectionCom";
    ViewData["Title"] = "Dashbord_Section_communication";
}
<div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Tables</h1>
    <p class="mb-4">
        DataTables is a third party plugin that is used to generate the demo table below.
        For more information about DataTables, please visit the <a target="_blank"
                                                                   href="https://datatables.net">official DataTables documentation</a>.
    </p>

    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Numero Affiliation</th>
                            <th>Nom Employeur</th>
                            <th>Nom Commune</th>
                            <th>Categorie Employeur</th>
                            
                        </tr>
                    </thead>
                         <tbody>
                        @foreach (var ab in Model)
                        {
                             <tr>
                                 <td>@ab.numero_Affiliation</td>
                                 <td>@ab.nom_Employeur</td>
                                 <td>@ab.nom_commune</td>
                                 <td>@ab.nom_TypeE</td>
                            

                             </tr>
                        }


                        
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>